/*
	Tutku Gündüzlü
	Fibonacci serisi
*/
#include<stdio.h>
#include<conio.h>

   int main()
   {   
      int a[10];
       int i,j,toplam=0;
   
   a[0]=0;
   a[1]=1;
   a[2]=1;
   printf("%d\t",a[0]);
   printf("%d\t",a[1]);
   printf("%d\t",a[2]);
   
                for(i=3;i<10;i++) 
				{
                	a[i]=a[i-2]+a[i-1];
                    printf("%d\t",a[i]);
                }
                printf("...");
                                  
                          
   getch();
   return 0;
   }
